{
  "source": {
    "include": [
      "./utils",
      "./antd"
    ],
    "exclude": [
      "./utils/route-utils.js"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md",
    "template": "node_modules/docdash"
  },
  "docdash": {
    "sort": false
  }
}